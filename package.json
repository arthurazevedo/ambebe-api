{
  "name": "ambebe-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/arthurazevedo/ambebe-api.git",
  "author": "arthurazevedo <arthur.stevam.costa@ccc.ufcg.edu.br>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpileOnly --ignore-watch node_modules --no-notify src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "yarn build && node dist/server.js",
    "migrate": "knex migrate:latest",
    "unmigrate": "knex migrate:rollback",
    "seed": "knex seed:run"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "pg": "^8.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.1",
    "ts-node-dev": "^1.0.0-pre.50",
    "typescript": "^3.9.6"
  }
}
